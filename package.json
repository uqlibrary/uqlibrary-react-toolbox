{
  "name": "uqlibrary-react-toolbox",
  "version": "0.0.27",
  "main": "build/index.js",
  "license": "MIT",
  "jest": {
    "testMatch": [
      "**/?(*.)(test).js?(x)"
    ],
    "transform": {
      ".*": "<rootDir>/node_modules/babel-jest"
    },
    "moduleFileExtensions": [
      "js",
      "json",
      "es6"
    ],
    "unmockedModulePathPatterns": [
      "react",
      "enzyme"
    ],
    "collectCoverage": true,
    "modulePaths": [
      "<rootDir>/src"
    ],
    "moduleNameMapper": {
      "\\.(css|scss|sass)$": "<rootDir>/__mocks__/styles.js",
      "\\.(jpg|jpeg|svg)$": "<rootDir>/__mocks__/images.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/uqlibrary/uqlibrary-react-toolbox.git"
  },
  "keywords": [
    "webpack",
    "plugin",
    "html",
    "html-webpack-plugin",
    "preloader",
    "browser-check"
  ],
  "author": "UQ Library",
  "bugs": {
    "url": "https://github.com/uqlibrary/uqlibrary-react-toolbox/issues"
  },
  "homepage": "https://github.com/uqlibrary/uqlibrary-react-toolbox",
  "semistandard": {
    "ignore": []
  },
  "devDependencies": {
    "autoprefixer": "^7.1.2",
    "axios-mock-adapter": "^1.9.0",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-eslint": "8.0.0",
    "babel-jest": "21.0.2",
    "babel-loader": "7.1.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-react-constant-elements": "6.23.0",
    "babel-plugin-transform-react-remove-prop-types": "0.4.8",
    "babel-polyfill": "6.26.0",
    "babel-preset-env": "1.6.0",
    "babel-preset-react": "6.24.1",
    "babel-preset-react-hmre": "1.1.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-preset-stage-1": "6.24.1",
    "chalk": "^2.0.1",
    "cross-env": "^5.0.1",
    "css-loader": "^0.28.1",
    "enzyme": "^2.9.1",
    "enzyme-to-json": "2.0.0",
    "eslint": "^4.5.0",
    "eslint-config-airbnb": "^15.0.2",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.1.0",
    "expect": "21.1.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "favicons-webpack-plugin": "0.0.7",
    "file-loader": "^0.11.2",
    "html-webpack-plugin": "^2.29.0",
    "ignore-styles": "^5.0.1",
    "jest": "21.1.0",
    "jest-cli": "^21.1.0",
    "jsdom": "^11.1.0",
    "jsdom-global": "^3.0.2",
    "node-sass": "^4.5.3",
    "np": "^2.16.0",
    "null-loader": "^0.1.1",
    "postcss-loader": "^2.0.6",
    "progress-bar-webpack-plugin": "^1.10.0",
    "query-string": "^5.0.0",
    "react-addons-test-utils": "^15.6.0",
    "react-hot-loader": "3.0.0-beta.6",
    "react-tap-event-plugin": "^2.0.1",
    "react-test-renderer": "^15.6.1",
    "redux-devtools": "^3.3.2",
    "redux-devtools-dock-monitor": "^1.1.1",
    "redux-devtools-log-monitor": "^1.2.0",
    "redux-mock-store": "^1.2.2",
    "sass-loader": "^6.0.6",
    "sass-variable-loader": "^0.1.1",
    "sinon": "^3.2.1",
    "style-loader": "^0.18.2",
    "watch": "^1.0.2",
    "webpack": "^3.2.0",
    "webpack-dev-server": "^2.5.1",
    "webpack-node-externals": "^1.6.0"
  },
  "dependencies": {
    "@material/animation": "^0.3.1",
    "axios": "^0.16.2",
    "bulma": "^0.6.0",
    "highcharts": "^5.0.10",
    "immutable": "^3.8.1",
    "material-ui": "^0.18.0",
    "moment": "^2.17.1",
    "moment-timezone": "^0.5.11",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-dropzone": "^4.1.0",
    "react-redux": "^5.0.2",
    "react-router": "^4.0.0",
    "react-router-dom": "^4.0.0",
    "redux": "^3.7.1",
    "timezones.json": "^1.2.1"
  },
  "peerDependencies": {
    "@material/animation": "^0.3.1",
    "bulma": "^0.6.0",
    "highcharts": "^5.0.10",
    "immutable": "^3.8.1",
    "material-ui": "^0.18.0",
    "moment": "^2.17.1",
    "moment-timezone": "^0.5.11",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-dropzone": "^4.1.0",
    "react-redux": "^5.0.2",
    "react-router": "^4.0.0",
    "react-router-dom": "^4.0.0",
    "redux": "^3.7.1",
    "timezones.json": "^1.2.1"
  },
  "scripts": {
    "test": "NODE_ENV=test jest",
    "test:update": "npm test --updateSnapshot",
    "test:watch": "npm test -- --watch",
    "build": "NODE_ENV=production babel src -d build --ignore '**/*.test.js'",
    "prepublishOnly": "npm run build",
    "dev": "watch 'npm run build' src",
    "release": "np",
    "eslint": "eslint 'src/**/*.js' --quiet"
  },
  "engines": {
    "npm": ">=3",
    "node": ">=6.2.2"
  },
  "babel": {
    "presets": [
      "env",
      "react",
      "stage-0",
      "stage-1"
    ],
    "env": {
      "development": {
        "presets": [
          "env",
          "react-hmre"
        ]
      },
      "production": {
        "presets": [
          [
            "env",
            {
              "es2015": {
                "modules": false
              },
              "targets": {
                "ie": 11,
                "uglify": true
              }
            }
          ]
        ],
        "plugins": [
          "transform-react-constant-elements",
          "transform-react-remove-prop-types",
          "transform-class-properties"
        ]
      },
      "test": {
        "presets": [
          "env",
          "react",
          "stage-0"
        ]
      }
    }
  }
}
